class Solution:
    def reverseVowels(self, s: str) -> str:
        left=0
        right=len(s)-1
        ss=list(s)
        vowels="aeiouAEIOU"
        while left<right:
            while left<right and ss[left] not in vowels:
                left+=1
            while left<right and ss[right] not in vowels:
                right-=1
            ss[left],ss[right]=ss[right],ss[left]
            left+=1
            right-=1
        return "".join(ss)
